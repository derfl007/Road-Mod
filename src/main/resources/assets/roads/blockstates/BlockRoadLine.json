{
    "multipart": [
        {   "apply": { "model": "roads:road_line_base" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "roads:road_line_north_white" }
        },
        {   "when": { "north": "false" },
            "apply": { "model": "roads:road_line_north" }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "roads:road_line_south_white" }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "roads:road_line_south" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "roads:road_line_east_white" }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "roads:road_line_east" }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "roads:road_line_west_white" }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "roads:road_line_west" }
        }
    ]
}